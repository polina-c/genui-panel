{
  "name": "genui-panel",
  "displayName": "genui-panel",
  "description": "Flutter generative UI",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "genui-panel.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "genui-panel.openview",
        "title": "Sidebar View"
      },
      {
        "command": "genui-panel.menu.view",
        "category": "vscode-extension-sidebar-html",
        "title": "Sample WebView in VS Code Sidebar",
        "icon": "$(clear-all)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "Gen UI :)",
          "icon": "assets/logo_bito.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "genui-panel.openview",
          "name": "View",
          "contextualTitle": "View"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "genui-panel.menu.view",
          "group": "navigation",
          "when": "view == genui-panel.openview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@vscode/debugadapter": "^1.61.0",
    "@vscode/debugprotocol": "^1.61.0",
    "minimatch": "^9.0.5",
    "semver": "^7.6.2",
    "vscode-languageclient": "^8.1.0",
    "vscode-uri": "^3.0.8",
    "ws": "^8.18.0",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/semver": "^7.5.8",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.11.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.2",
    "vscode-uri": "^3.0.8"
  }
}
